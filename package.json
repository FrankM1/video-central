    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-sass": "~1.0.0",
    "grunt-contrib-uglify": "^3.3.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-dev-update": "^2.3.0",
    "grunt-exorcise": "~2.1.1",
    "grunt-imagine": "^0.3.6",
    "grunt-postcss": "~0.9.0",
    "grunt-wp-i18n": "^1.0.0",
    "jit-grunt": "^0.10.0",
